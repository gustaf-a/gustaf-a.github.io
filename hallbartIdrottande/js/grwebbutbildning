# ServerName gro.vps-56841.cloudnet.se
# ServerAlias gro.pedagogisktcentrum.se
# Cloudnet default virtual host
# Matches both 80 and 88 to prepare for Varnish
<VirtualHost *:80 *:88>
        ServerAdmin webmaster@localhost

	ServerName www.grtest.se
        ServerAlias *.grtest.se
	ServerAlias grwebbutbildning.vps-56841.cloudnet.se
        ServerAlias www.grtest.se


        DocumentRoot /mnt/persist/www/grwebbutbildning/
        <Directory />
                Options FollowSymLinks
                AllowOverride All
        </Directory>
        <Directory /mnt/persist/www/grwebbutbildning/>
                Options FollowSymLinks
                AllowOverride All
                Order allow,deny
                allow from all
        </Directory>

        ErrorLog /var/log/apache2/error.log

        # Possible values include: debug, info, notice, warn, error, crit,
        # alert, emerg.
        LogLevel warn

        CustomLog /var/log/apache2/access.log vhost_combined

</VirtualHost>

